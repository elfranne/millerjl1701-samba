# Managed by Puppet.

<% @config.map do |k,v| -%>
<%   if v.is_a?(Hash) -%>
[<%=  k %>]
<%     v.map do |ki, vi| -%>
<%       if vi.is_a?(Array) -%>
<%         if ki.start_with?(';') -%>
;	 <%=        ki[1..-1] %> = <%= vi.join(' ') %>
<%         else -%>
	 <%=        ki %> = <%= vi.join(' ') %>
<%         end -%>
<%       elsif vi != :undef -%>
<%         if ki.start_with?(';') -%>
;	 <%=        ki[1..-1] %> = <%= vi %>
<%         else -%>
	 <%=        ki %> = <%= vi %>
<%         end -%>
<%       end -%>
<%     end -%>

<%   else -%>
[<%=  k %>]

<%   end -%>
<% end -%>
